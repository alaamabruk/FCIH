<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Servalet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>appcontroller</servlet-name>
        <servlet-class>Servalet.appcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminDeleteCourse</servlet-name>
        <servlet-class>Servalet.adminDeleteCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addcourse</servlet-name>
        <servlet-class>Servalet.Addcourse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>appcontroller</servlet-name>
        <url-pattern>/appcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminDeleteCourse</servlet-name>
        <url-pattern>/adminDeleteCourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addcourse</servlet-name>
        <url-pattern>/Addcourse</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
